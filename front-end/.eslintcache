ccounts}\n\t\t\t\t\t\t\taddress={address}\n\t\t\t\t\t\t\tonAccountChange={onAccountChange}\n\t\t\t\t\t\t/>\n\t\t\t\t\t\t<AyeNayButtons\n\t\t\t\t\t\t\tdisabled={!isApiReady}\n\t\t\t\t\t\t\tonClickAye={() => voteMotion(true)}\n\t\t\t\t\t\t\tonClickNay={() => voteMotion(false)}\n\t\t\t\t\t\t/>\n\t\t\t\t\t</Card>\n\t\t\t}\n\t\t</>;\n\n\tconst NotCouncil = () =>\n\t\t<Card>\n\t\t\t<h3>Vote</h3>\n\t\t\t<div>No account found from the council :(</div>\n\t\t\t<ButtonLink onClick={() => setForceVote(true)}>Let me try still.</ButtonLink>\n\t\t</Card>;\n\n\treturn (\n\t\t<div className={className}>\n\t\t\t{isCouncil || forceVote\n\t\t\t\t? <VotingForm/>\n\t\t\t\t: <NotCouncil/>\n\t\t\t}\n\t\t</div>\n\t);\n};\n\nexport default styled(VoteMotion)`\n\t.LoaderWrapper {\n\t\theight: 15rem;\n\t\tposition: absolute;\n\t\twidth: 100%;\n\t}\n`;\n","/home/thib/github/paritytech/polkassembly/front-end/src/components/Post/GovernanceSideBar/Motions/MotionVoteInfo.tsx",[],"/home/thib/github/paritytech/polkassembly/front-end/src/components/Post/Poll/Poll.tsx",[],"/home/thib/github/paritytech/polkassembly/front-end/src/ui-components/UpdateLabel.tsx",[],"/home/thib/github/paritytech/polkassembly/front-end/src/components/Comment/EditableCommentContent.tsx",[],"/home/thib/github/paritytech/polkassembly/front-end/src/components/Post/GovernanceSideBar/Proposals/index.tsx",[],"/home/thib/github/paritytech/polkassembly/front-end/src/ui-components/AyeNayButtons.tsx",[],"/home/thib/github/paritytech/polkassembly/front-end/src/ui-components/VoteProgress.tsx",[],"/home/thib/github/paritytech/polkassembly/front-end/src/ui-components/BalanceInput.tsx",[],"/home/thib/github/paritytech/polkassembly/front-end/src/ui-components/PassingInfo.tsx",[],"/home/thib/github/paritytech/polkassembly/front-end/src/components/Post/Poll/GeneralSignals.tsx",[],"/home/thib/github/paritytech/polkassembly/front-end/src/components/Post/Poll/CouncilSignals.tsx",[],"/home/thib/github/paritytech/polkassembly/front-end/src/util/cop